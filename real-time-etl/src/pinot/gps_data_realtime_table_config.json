{
  "tableName": "gpsdata",
  "tableType": "REALTIME",
  "segmentsConfig": {
    "timeColumnName": "timestamp",
    "schemaName": "gpsdata",
    "replication": "1",
    "replicasPerPartition": "1"
  },
  "tenants": {},
  "tableIndexConfig": {
    "loadMode": "MMAP",
    "streamConfigs": {
      "streamType": "kafka",
      "stream.kafka.broker.list": "kafka:9092",
      "stream.kafka.consumer.type": "lowlevel",
      "stream.kafka.topic.name": "wikipedia-events",
      "stream.kafka.decoder.prop.format": "JSON",
      "stream.kafka.consumer.factory.class.name": "org.apache.pinot.plugin.stream.kafka20.KafkaConsumerFactory",
      "stream.kafka.decoder.class.name": "org.apache.pinot.plugin.stream.kafka.KafkaJSONMessageDecoder",
      "stream.kafka.consumer.prop.auto.offset.reset": "smallest"
    }
  },
  "metadata": {},
  "quota": {},
  "routing": {},
  "query": {},
  "ingestionConfig": {
    "transformConfigs": [
      {
        "columnName": "metaJson",
        "transformFunction": "JSONFORMAT(meta)"
      },
      {
        "columnName": "timestamp",
        "transformFunction": "JSONPATH(metaJson, '$.timestamp')"
      },
      {
        "columnName": "lat",
        "transformFunction": "JSONPATH(metaJson, '$.lat')"
      },
      {
        "columnName": "lon",
        "transformFunction": "JSONPATH(metaJson, '$.lon')"
      },
      {
        "columnName": "fare_amount",
        "transformFunction": "JSONPATH(metaJson, '$.fare_amount')"
      },
      {
        "columnName": "tip_amount",
        "transformFunction": "JSONPATH(metaJson, '$.tip_amount')"
      }
    ]
  },
  "isDimTable": false
}
